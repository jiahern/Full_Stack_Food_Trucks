  <div class="signupin-section">
    <div class="login-container w-container">
      <div class="login-div">
        {{#if message}}
          <div style="color:red;text-align:center;" >{{message}}</div>
        {{/if}}
        <h1 data-w-id="d02b6f0e-0329-d7cf-4d14-f2641cd04e16" style="opacity:0;-webkit-transform:translate3d(0, 30px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 30px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 30px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 30px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)" class="heading-3">SIGN UP</h1>
        
        {{!-- Sign up form here --}}
        <div class="form-block w-form">
          <form method="POST" action="/customer/signup" id="wf-form-login-form" name="wf-form-login-form" data-name="login-form" data-w-id="d02b6f0e-0329-d7cf-4d14-f2641cd04e19" style="-webkit-transform:translate3d(0, 40px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 40px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 40px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 40px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);opacity:0" class="login-form">
              <label for="email" class="input-box-label1">User Email</label>
              <input name="email" type="email" class="signup-text-box w-input" maxlength="256" data-name="email" placeholder="Enter your email here.." id="email">
              <label for="password" class="input-box-label2">Password</label>
              <input name="password" type="password" pattern="(?=.*\d)(?=.*[a-z]|[A-Z]).{8,}" title="Password must contain at least 8 characters with at least one number and one alphabet character" class="signup-text-box w-input" maxlength="256" data-name="password" placeholder="Enter your password here..." id="password" required="">
              <label for="first-name" class="input-box-label3">First Name</label>
              <input name="firstname" type="text" class="signup-text-box w-input" maxlength="256"  data-name="first-name" placeholder="Enter your first name here..." id="first-name" required="">
              <label for="last-name" class="field-label">Last Name</label>
              <input name="lastname" type="text" class="signup-text-box w-input" maxlength="256"  data-name="last-name" placeholder="Enter your last name here..." id="last-name" required="">
              <input type="submit" value="Sign Up" data-wait="Please wait..." class="signin-btn signup-btn w-button">
          </form>
        </div>

      </div>
    </div>
  </div>

  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=60a214d75c9dd322bc62cd6d" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
