  <div class="signupin-section">
    <div class="login-container w-container">
      <div data-w-id="912447c7-8249-0b54-9637-773c35df987f" style="-webkit-transform:translate3d(0, 40px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 40px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 40px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 40px, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);opacity:0" class="login-div">
        {{#if message}}
          <div style="color:red;text-align:center;" >{{message}}</div>
        {{/if}}
        <h1 class="heading-3">SIGN IN</h1>

        {{!-- Sign in form here --}}
        <div class="w-form">
          <form method="POST" action="/customer/login" id="wf-form-login-form" name="wf-form-login-form" data-name="login-form" class="login-form">
            <label for="email-3" class="input-box-label1">User Email</label>
            <input name="email" type="email" class="signup-text-box w-input" maxlength="256"  data-name="email" placeholder="Enter you email here..." id="email-3">
            <label for="password" class="input-box-label2">Password</label>
            <input name="password" type="password" class="signup-text-box w-input" maxlength="256" data-name="password" placeholder="Enter you password here..." id="password" required="">
            {{!-- Link to jump to sign-up page if not registered --}}
            <a href="/customer/signup" class="not-registered-link">Haven&#x27;t registered?  Sign up here...</a>
            {{!-- Sign in btton --}}
            <input type="submit" value="Sign In" data-wait="Please wait..." class="signin-btn w-button">
          </form>
        </div>

      </div>
    </div>
